        "autoprefixer": "^10.4.20",
        "cypress": "^13.12.0",
        "daisyui": "^4.12.13",
        "eslint": "^8.57.0",
        "eslint-plugin-cypress": "^3.3.0",
        "eslint-plugin-vue": "^9.23.0",
        "npm-run-all2": "^6.2.0",
        "postcss": "^8.4.47",
        "prettier": "^3.2.5",
        "start-server-and-test": "^2.0.4",
        "tailwindcss": "^3.4.13",
        "typescript": "~5.4.0",
